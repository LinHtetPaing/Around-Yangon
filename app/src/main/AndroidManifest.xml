        <activity android:name=".activities.ShoppingCenter" />
        <activity android:name=".activities.University" />
        <activity android:name=".activities.CinemaDetails" />
        <activity android:name=".activities.RestaurantDetails" />
        <activity android:name=".activities.UniversityDetails" />
        <activity android:name=".activities.PagodasDetails" />
        <activity android:name=".activities.HospitalDetails" />
